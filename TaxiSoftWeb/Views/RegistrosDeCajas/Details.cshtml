@model TaxiSoftWeb.Models.RegistrosDeCaja

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Registro de Caja</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaRegisCaja)
        </dd>
        <dt class="col-sm-2">
            Caja
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdCajaNavigation.NomCaja)
        </dd>
        <dt class="col-sm-2">
            Operación
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdOperacionNavigation.NomOperacion)
        </dd>
        <dt class="col-sm-2">
            Importe
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Importe)
        </dd>
        <dt class="col-sm-2">
            Turno
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdTurnoNavigation.NomTurno)
        </dd>
        <dt class="col-sm-2">
            Movil
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdVehiculoNavigation.IdVehiculo)
        </dd>
        <dt class="col-sm-2">
            Cuil
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CuilNavigation.Cuil)
        </dd>
        <dt class="col-sm-2">
            Concepto
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Concepto)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdRegistroCaja">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
