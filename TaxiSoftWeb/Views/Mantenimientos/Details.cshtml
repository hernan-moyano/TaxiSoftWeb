@model TaxiSoftWeb.Models.Mantenimiento

@{
    ViewData["Title"] = "Details";
}
<h1>Detalles</h1>

<div>
    <h4>Mantenimiento</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Movil:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdVehiculoNavigation.IdVehiculo)
        </dd>
        <dt class="col-sm-2">
             Fecha de Mant.:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaMant)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Valor):
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Valor)
        </dd>
        <dt class="col-sm-2">
            Estado:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdEstadoANavigation.NomEstadoA)
        </dd>
        <dt class="col-sm-2">
            Descripción:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descripcion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdMant" class="btn btn-warning">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>

