@model TaxiSoftWeb.Models.Carnet

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Carnet</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Número de Carnet:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NroCarnet)
        </dd>
        <dt class = "col-sm-2">
            Vencimiento:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VtoCarnet)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdCarnet" class="btn btn-warning">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>
