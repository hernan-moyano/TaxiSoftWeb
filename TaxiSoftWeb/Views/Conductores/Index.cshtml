            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDomicilioNavigation.IdDomicilio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPuestoNavigation.IdPuesto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTurnoNavigation.IdTurno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdVehiculoNavigation.IdVehiculo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Cuil">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Cuil">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Cuil">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
