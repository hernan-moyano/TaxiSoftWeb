@model TaxiSoftWeb.Models.Itv

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Itv</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Vigencia Desde:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VigenciaDesde)
        </dd>
        <dt class = "col-sm-2">
            Vigencia Hasta:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VigenciaHasta)
        </dd>
        <dt class = "col-sm-2">
            Descripción:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descripcion)
        </dd>
        <dt class = "col-sm-2">
            Valor:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Valor)
        </dd>
        <dt class = "col-sm-2">
            Movil:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdVehiculoNavigation.IdVehiculo)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdItv" class="btn btn-warning">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>
